{
  "name": "better-pages-create",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "description": "Better react file routing",
  "author": "Chiko",
  "license": "MIT",
  "keywords": [
    "vite",
    "react",
    "react-router",
    "pages",
    "routing",
    "typescript"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "start": "pnpm --filter better-pages-create-react-example dev",
    "build": "turbo build --filter='./packages/*'",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "prepare": "husky",
    "release": "pnpm build && pnpm changeset publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "eslint": "^9.29.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "turbo": "^2.5.5"
  },
  "lint-staged": {
    "*.{css,html,json,js,jsx,ts,tsx,mdx}": [
      "eslint --fix"
    ]
  }
}
